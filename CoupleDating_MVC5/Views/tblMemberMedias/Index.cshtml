@model IEnumerable<CoupleDating_MVC5.Models.MemberMedia>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.path)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tblMediaType.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Member.FirstName1)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.path)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tblMediaType.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Member.FirstName1)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.mediaID }) |
            @Html.ActionLink("Details", "Details", new { id=item.mediaID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.mediaID })
        </td>
    </tr>
}
</table>