@model CoupleDating_MVC5.Models.Favorites

@using (Html.BeginForm("AddFavorite","Favorites"))
{
    @Html.AntiForgeryToken()

    if (Model == null || Model.enabled == false)
    {
        <input type="submit" value="" name="AddFavorite" class=""
               style="background-image:url(../../Content/images/Favorites.png) ; width:50px; height:50px;" />
    <span>Click to add as Favorite</span>

    }
    else
    {
        <input type="submit" value="" name="AddFavorite" class=""
               style="background-image:url(../../Content/images/Favorites_Added.png) ; width:50px; height:50px;" />
        <span>Added as Favorite</span>
    }

}