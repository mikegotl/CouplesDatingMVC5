@model IEnumerable<CoupleDating_MVC5.Models.QuestionAnswers>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            First Name
        </th>
        <th>
            Choice Full
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Member.FirstName1)
        </td>
        <td>
            @{

                @Html.DropDownListFor(m => item.questionChoiceID, new SelectList(ViewBag.questionChoices, "questionChoiceID", "questionChoiceFull"), htmlAttributes: new { @class = "form-control" });
            }
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.questionAnswerID }) |
            @Html.ActionLink("Details", "Details", new { id=item.questionAnswerID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.questionAnswerID })
        </td>
    </tr>
}
</table>